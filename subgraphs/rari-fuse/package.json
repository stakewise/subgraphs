{
  "name": "rari-fuse",
  "version": "0.1.2",
  "license": "AGPL-3.0-only",
  "scripts": {
    "codegen": "graph codegen subgraph.yaml",
    "build": "graph build subgraph.yaml",
    "prepare:goerli": "true",
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
    "create:local": "graph create --node $GRAPH_NODE_URL stakewise/rari-fuse",
    "deploy:mainnet": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ stakewise/rari-fuse-mainnet",
    "deploy:goerli": "true",
    "deploy:local": "graph deploy --version-label 0.1.2 --node $GRAPH_NODE_URL --ipfs $IPFS_URL stakewise/rari-fuse"
  }
}
